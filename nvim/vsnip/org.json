{
  "begin_src": {
    "prefix": ["beg", "#+beg"],
    "body": [
      "#+begin_src${1}",
      "$0",
      "#+end_src"
    ],
    "description": "#+begin_src template."
  },
  "begin_quote": {
    "prefix": ["quo", "#+quo"],
    "body": [
      "#+begin_quote",
      "$0",
      "#+end_quote"
    ],
    "description": "#+begin_quote template."
  },
  "block_math": {
    "prefix": ["\\["],
    "body": [
      "\\[",
      "$0",
      "\\]"
    ],
    "description": "block math template."
  },
  "inline_math": {
    "prefix": ["\\("],
    "body": [
      "\\( $1 \\)$0"
    ],
    "description": "inline math template."
  },
  "frac": {
    "prefix": ["frac"],
    "body": [
      "\\frac{$1}{$2}$0"
    ],
    "description": "fraction."
  }
}
